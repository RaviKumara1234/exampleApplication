<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    android:background="#F5F5F5">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/cuisineTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Non-Vegetarian Cuisine"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:layout_marginBottom="16dp"/>

        <!-- Image 1: Chicken Tikka -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <ImageView
                android:id="@+id/foodImage1"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:src="@drawable/chickentikka"
                android:layout_marginBottom="8dp"
                android:scaleType="centerCrop"
                android:contentDescription="chickentikka"/>

            <TextView
                android:id="@+id/foodName1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Chicken Tikka"
                android:textSize="14sp"
                android:textColor="@android:color/black"
                android:gravity="center"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>

        <!-- Image 2: Butter Chicken -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <ImageView
                android:id="@+id/foodImage2"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:src="@drawable/butterchicken"
                android:layout_marginBottom="8dp"
                android:scaleType="centerCrop"
                android:contentDescription="butterchicken"/>

            <TextView
                android:id="@+id/foodName2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Butter Chicken"
                android:textSize="14sp"
                android:textColor="@android:color/black"
                android:gravity="center"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>

        <!-- Image 3: Mutton Kebab -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <ImageView
                android:id="@+id/foodImage3"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:src="@drawable/muttonkebab"
                android:layout_marginBottom="8dp"
                android:scaleType="centerCrop"
                android:contentDescription="muttonkebab"/>

            <TextView
                android:id="@+id/foodName3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
